{
  "name": "kuali-cor-workflows-common",
  "version": "0.4.26",
  "description": "Workflows code common to both client and server",
  "author": "Kuali Core Team",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/KualiCo/cor-workflows-common/issues"
  },
  "homepage": "https://github.com/KualiCo/cor-workflows-common#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp",
    "prepublish": "npm run onlymaster && npm test && npm run build",
    "test": "standard && jest",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "onlymaster": "[[ \"$(git rev-parse --abbrev-ref HEAD)\" == \"master\" ]] || (echo 'You can only publish from the master branch. You should also be a core team member' && exit 1)",
    "watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/KualiCo/cor-workflows-common.git"
  },
  "keywords": [
    "kuali",
    "core",
    "workflows"
  ],
  "standard": {
    "env": [
      "browser",
      "node",
      "jest"
    ],
    "ignore": [
      "coverage",
      "lib"
    ],
    "parser": "babel-eslint"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "html",
      "json",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 89,
        "branches": 82,
        "lines": 89,
        "functions": 89
      }
    },
    "rootDir": "src",
    "verbose": true
  },
  "dependencies": {
    "axios": "0.18.0",
    "bluebird": "3.5.1",
    "lodash": "4.17.5",
    "querystring": "0.2.0",
    "shorthash": "0.0.2"
  },
  "devDependencies": {
    "autoprefixer": "8.2.0",
    "axios-mock-adapter": "1.15.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "gulp": "3.9.1",
    "gulp-babel": "7.0.1",
    "gulp-concat": "2.6.1",
    "gulp-postcss": "7.0.1",
    "jest": "22.4.3",
    "jest-cli": "22.4.3",
    "postcss-cli": "5.0.0",
    "postcss-modules": "1.1.0",
    "rimraf": "2.6.2",
    "standard": "11.0.1"
  }
}
