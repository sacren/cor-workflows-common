{
  "name": "kuali-cor-workflows-common",
  "version": "0.3.13",
  "description": "Workflows code common to both client and server",
  "author": "Kuali Core Team",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/KualiCo/cor-workflows-common/issues"
  },
  "homepage": "https://github.com/KualiCo/cor-workflows-common#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp",
    "prepublish": "npm test && npm run build",
    "test": "standard && jest",
    "test:jest:watch": "jest --watch",
    "watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/KualiCo/cor-workflows-common.git"
  },
  "keywords": [
    "kuali",
    "core",
    "workflows"
  ],
  "standard": {
    "env": [
      "browser",
      "node",
      "jest"
    ],
    "ignore": [
      "coverage",
      "lib"
    ],
    "parser": "babel-eslint"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "html",
      "json",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 92,
        "branches": 88,
        "lines": 93,
        "functions": 90
      }
    },
    "mapCoverage": true,
    "rootDir": "src",
    "verbose": true
  },
  "dependencies": {
    "axios": "0.17.1",
    "bluebird": "3.5.1",
    "lodash": "4.17.4",
    "querystring": "0.2.0",
    "shorthash": "0.0.2"
  },
  "devDependencies": {
    "autoprefixer": "7.2.5",
    "axios-mock-adapter": "1.12.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.1.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "gulp": "3.9.1",
    "gulp-babel": "7.0.1",
    "gulp-concat": "2.6.1",
    "gulp-postcss": "7.0.1",
    "jest": "22.1.4",
    "jest-cli": "22.1.4",
    "postcss-cli": "4.1.1",
    "postcss-modules": "1.1.0",
    "rimraf": "2.6.2",
    "standard": "10.0.3"
  }
}
